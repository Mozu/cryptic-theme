{
  "about": {
    "name": "Cryptic Theme vpinterestwidget",
    "author": "You",
    "extends": "Core4",
    "isDesktop": true,
    "isMobile": true,
    "defaultLanguage": "en-US",
    "version": "pinterestwidget"
  },
  "settings": {
    "bannerImageEnabled": true,
    "bannerImage": "../resources/images/cryptic-header.png",
    "listProductThumbSize": 300
  },
  "emailTemplates": [],
  "pageTypes": [],
  "widgets": [
    {
      "id": "pin_this",
      "displayName": "Pin This",
      "displayTemplate": "catalog/pin-this",
      "editViewFields": [
        {
          "name": "shape",
          "fieldLabel": "Shape",
          "xtype": "combo",
          "store": [
            [
              "round",
              "Round"
            ],
            [
              "rectangular",
              "Rectangular"
            ]
          ]
        },
        {
          "name": "size",
          "fieldLabel": "Size",
          "xtype": "combo",
          "store": [
            [
              "small",
              "Small"
            ],
            [
              "large",
              "Large"
            ]
          ]
        }
      ],
      "icon": "/resources/admin/widgets/pinterest.png",
      "validPageTypes": [
        "product"
      ]
    }
  ]
}